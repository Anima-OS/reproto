use foo.bar@^2;
use foo as bar2;

tuple Tuple {
  arg: double;
  second: double;
}

type Foo {
  reserved "foo";

  name: string;
  map: {string: unsigned/32};

  java {{
    public int hello() {
      return 42;
    }
  }}
}

interface Bar {
  reserved "a";

  java {{
    default int hello() {
      return 42;
    }
  }}

  A {
      name "foo";
      name: string;
  }
}

enum Baz {
  FIRST as "first";
  SECOND as "second";
  FIRST as "third";
}

